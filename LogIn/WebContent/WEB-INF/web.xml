<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LogIn</display-name>
  <welcome-file-list>
    <welcome-file>First</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>com.bridgelabz.Register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.bridgelabz.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditDetails</servlet-name>
    <servlet-class>com.bridgelabz.EditDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditDetails</servlet-name>
    <url-pattern>/EditDetails</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogOut</servlet-name>
    <servlet-class>com.bridgelabz.LogOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogOut</servlet-name>
    <url-pattern>/LogOut</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Viewall</servlet-name>
    <servlet-class>com.bridgelabz.Viewall</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Viewall</servlet-name>
    <url-pattern>/Viewall</url-pattern>
  </servlet-mapping>
</web-app>